from sklearn.preprocessing import StandardScaler

data=pd.read_csv("/content/gdrive/MyDrive/temp/data.csv", encoding= 'big5')# 地址自己改

#用於增益
for i in range(0,10):
  data=data.append(data.iloc[0,:],ignore_index=True)


X=data.iloc[:,0:5]
Y=data.iloc[:,5:]

from keras.models import Sequential
from keras.layers import Dense
model.add(Dense(32, input_shape=(data.shape[1]-2,), activation="relu"))
model.add(Dense(16, activation="relu"))
model.add(Dense(2))
model.compile(loss="mse", optimizer="adam",metrics=["mae"])
model.fit(X, Y, epochs=150, batch_size=10)

#如要預測執行以下
model.predict(X.iloc[7:8,:]) 
